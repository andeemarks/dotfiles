# start rofi (a dmenu substitute)
bindsym $mod+d exec --no-startup-id rofi -show drun -run-command "i3-msg exec '{cmd}'" -show-icons -theme arthur
bindsym $mod+Shift+d exec --no-startup-id rofi -show run -run-command "i3-msg exec '{cmd}'" -show-icons -theme arthur
bindsym $mod+Tab exec --no-startup-id rofi -show window -show-icons -theme arthur
